                "defaults": {
                    "custom": {}
                },
                "overrides": []
            },
            "gridPos": {
                "h": 10,
                "w": 24,
                "x": 0,
                "y": 0
            },
            "id": 35,
            "mode": "markdown",
            "title": "Panel Title",
            "type": "text"
        },
